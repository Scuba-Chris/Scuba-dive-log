<!DOCTYPE html>
<html lang="en">

<head>
    <%- include('./partials/head') %>
</head>

<body>
    <header>
        <%- include('./partials/header') %>
    </header>
    <main>
        <nav>
            <%- include('./partials/nav') %>
        </nav>

        <h2>Dive History</h2>
        <p> #
            <%= dives.length %> Dives </p>
        <% dives.forEach(dive => { %>
            <article class="dives_sections">
                <%- include('./partials/eachDive', {dives : dive}) %>
            </article>
            <% }) %>

    </main>

    <footer>
        <%- include('./partials/footer') %>
    </footer>

</body>

</html>